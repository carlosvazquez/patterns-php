{
    "name": "php/pattern",
    "description": "Patterns in PHP POO",
    "keywords": ["php", "poo", "patterns", "adapter"],
    "license": "MIT",
    "type": "learning",
    "require": {
        "php": ">=8.0.0"
    },
    "autoload": {
        "classmap": [
        ],
        "files": [
        ],
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests"
        }
    },
    "extra": {
    },
    "scripts": {
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
