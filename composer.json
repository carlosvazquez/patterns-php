{
    "name": "carlosvazquez/phppatterns",
    "description": "PHP Patterns",
    "keywords": ["php", "poo", "patterns", "decorator"],
    "type": "learning",
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Carlos Vazquez",
            "email": "carlosvazquez@outlook.com"
        }
    ],
    "extra": {
    },
    "scripts": {
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
        ],
        "files": [
        ],
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=8.0.0"
    }
}
